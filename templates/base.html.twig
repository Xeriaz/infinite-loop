<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ "My project" }}{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/css/app.css') }}">
        {% endblock %}
        <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico')}}">
        {% block canonical_meta %}{% endblock %}
    </head>
    <body>

    {% if is_granted('ROLE_USER') %}
        {% include 'includes/header.html.twig' %}

        <div class="content">
            {% include 'includes/sideNav.html.twig' %}
            {% block content %}{% endblock %}
        </div>

        {% block javascripts %}
        <script src="{{ asset('build/js/app.js') }}"></script>
        {% endblock %}

    {% else %}

        <div class="wrapperLanding">
            <section class="landingPage">

                {#<a class="btn-inline mr-3" href="{{ path('fos_user_registration_register') }}">Sign Up</a>#}
                {#<a class="btn-inline" href="{{ path('fos_user_security_login') }}">Login</a>#}
                {% block body %}{% endblock %}


            </section>
        </div>





    {% endif %}



    </body>
</html>
